<service name="HelloWorldService" scope="application">
    <description>Hello World Sample Service</description>
    <messageReceivers>
        <messageReceiver
                mep="http://www.w3.org/ns/wsdl/in-only"
                class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver"/>
        <messageReceiver
                mep="http://www.w3.org/ns/wsdl/in-out"
                class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </messageReceivers>
    <parameter name="ServiceClass">nrw.it.training.HelloWorldService</parameter>
</service>